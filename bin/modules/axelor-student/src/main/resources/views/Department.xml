<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

	<form model="com.axelor.student.db.Department"
		title="Department" name="student-department-form">
		<field name="departmentName"></field>
	</form>
	
	<!-- this 2 is for side menu item -->
	
	<grid name="department-grid" editable="true" title="Departments"
		model="com.axelor.student.db.Department">
		<field name="departmentName" title="Department Name" />
	</grid>

	<form model="com.axelor.student.db.Department"
		title="Department" name="department-form">
		<field name="departmentName" title="Department Name"></field>
	</form>
	
	<action-view name="Department view" title="Add Department"
		model="com.axelor.student.db.Department">
		<view type="form" name="department-form" />
		<view type="grid" name="department-grid" />
	</action-view>

</object-views>